{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "scripts": {
    "generate-types": "typechain --target=ethers-v5 'abi/*.json' --out-dir generated/typechain",
    "generate": "yarn generate-types && yarn mst-gql:generate",
    "postinstall": "yarn generate",
    "dev": "npx next",
    "build": "next build && next-sitemap",
    "test": "jest -w 1",
    "test:ci": "yarn generate && yarn test",
    "start": "next start",
    "lint": "eslint . --max-warnings 0 --ext ts,tsx",
    "lint:fix": "yarn lint --fix",
    "mst-gql:generate": "./internals/mst-gql-generate.bash"
  },
  "dependencies": {
    "@prepo-io/stores": "0.23.0",
    "@sentry/nextjs": "6.19.6",
    "antd": "4.19.5",
    "bnc-onboard": "1.38.2",
    "clone-deep": "4.0.1",
    "configcat-js-ssr": "3.10.0",
    "ethereum-multicall": "2.10.0",
    "ethers": "5.6.3",
    "graphql": "16.3.0",
    "graphql-tag": "2.12.6",
    "mobx": "6.6.0",
    "mobx-react-lite": "3.3.0",
    "mobx-state-tree": "5.1.3",
    "mst-gql": "0.14.1",
    "next": "12.1.6",
    "next-sitemap": "2.5.19",
    "react": "17.0.2",
    "react-copy-to-clipboard": "5.0.4",
    "react-dom": "17.0.2",
    "react-jazzicon": "0.1.3",
    "react-use": "17.3.2",
    "styled-components": "5.3.5",
    "prepo-constants": "*",
    "prepo-utils": "*"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "10.0.0",
    "@types/react": "17.0.45",
    "@types/react-copy-to-clipboard": "5.0.2",
    "@types/styled-components": "5.1.25",
    "config": "*",
    "tsconfig": "*",
    "typechain": "8.0.0",
    "next-transpile-modules": "9.0.0"
  },
  "resolutions": {
    "@types/react": "17.0.45"
  }
}
