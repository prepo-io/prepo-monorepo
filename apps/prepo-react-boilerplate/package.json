{
  "name": "prepo-react-boilerplate",
  "version": "1.0.0",
  "scripts": {
    "generate-types": "typechain --target=ethers-v5 'abi/*.json' --out-dir generated/typechain",
    "postinstall": "yarn generate-types && yarn mst-gql:generate",
    "dev": "npx next",
    "build": "next build && next-sitemap",
    "test": "jest -w 1",
    "start": "next start",
    "lint": "eslint . --max-warnings 0 --ext ts,tsx",
    "lint:fix": "yarn lint --fix",
    "mst-gql:generate": "./internals/mst-gql-generate.bash"
  },
  "dependencies": {
    "@prepo-io/constants": "0.13.0",
    "@prepo-io/stores": "0.23.0",
    "@prepo-io/utils": "0.10.0",
    "@sentry/nextjs": "6.19.6",
    "antd": "4.19.5",
    "bnc-onboard": "1.38.2",
    "clone-deep": "4.0.1",
    "configcat-js": "5.7.2",
    "ethereum-multicall": "2.10.0",
    "ethers": "5.6.3",
    "graphql": "16.3.0",
    "graphql-tag": "2.12.6",
    "mobx": "6.5.0",
    "mobx-react-lite": "3.3.0",
    "mobx-state-tree": "5.1.3",
    "mst-gql": "0.14.1",
    "next": "12.1.5",
    "next-sitemap": "2.5.19",
    "react": "17.0.2",
    "react-copy-to-clipboard": "5.0.4",
    "react-dom": "17.0.2",
    "react-jazzicon": "0.1.3",
    "react-use": "17.3.2",
    "styled-components": "5.3.5"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "7.1.1",
    "@types/jest": "27.0.2",
    "@types/react": "17.0.37",
    "@types/react-copy-to-clipboard": "5.0.2",
    "@types/styled-components": "5.1.15",
    "babel-jest": "27.2.1",
    "config": "*",
    "eslint": "8.12.0",
    "jest": "27.2.1",
    "prettier": "2.4.1",
    "ts-jest": "27.1.2",
    "ts-node": "10.4.0",
    "tsconfig": "*",
    "typechain": "5.1.2",
    "typescript": "4.5.2"
  }
}
